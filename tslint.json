{
  "rulesDirectory": ["tslint-plugin-prettier"],
  "extends": ["tslint-config-standard", "tslint-config-prettier"],
  "rules": {
    //"no-unused-variable": true,
    "prettier": [
      true,
      {
        "singleQuote": true,
        "semi": false,
        "trailingComma": "es5",
        "printWidth": 120
      }
    ]
  }
}
